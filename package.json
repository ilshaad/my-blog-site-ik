{
  "name": "my-blog-site-ik",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ikclean": "rm -fr node_modules/ package-lock.json .next/ && npm cache clean -vf && npm i && npm update && npm audit fix --force && npm outdated",
    "ikclean2": "rm -fr node_modules/ package-lock.json .next/ && npm cache clean -vf && npm i && npm outdated",
    "ikcleanci": "rm -fr node_modules/ .next/ && npm cache clean -vf && npm ci && npm outdated",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "server": "npm run build && npm run start"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bootstrap": "^5.3.6",
    "next": "^15.3.2",
    "next-pwa": "^5.6.0",
    "normalize.css": "^8.0.1",
    "react": "19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "eslint": "^9.27.0",
    "eslint-config-next": "^15.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sass": "^1.89.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
